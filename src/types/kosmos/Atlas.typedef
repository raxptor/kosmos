#include Texture.typedef

AtlasEntry non-instantiable
{
	string id
	float u0
	float v0
	float u1
	float v1
}

AtlasOutput non-instantiable
{
	int Width
	int Height
	float Scale
	AtlasEntry[] Entries
	ptr Texture Texture
}

Atlas
{
	ptr[] Texture Inputs
	[no-out] ptr TextureConfiguration OutputConfiguration
	[hidden] AtlasOutput[] Outputs
}
